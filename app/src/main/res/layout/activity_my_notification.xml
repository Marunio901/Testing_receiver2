<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.patryko.testing_receiver2.MyNotification">

    <TextView
        android:id="@+id/activity_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="62dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="137dp"
        android:layout_marginStart="137dp"
        android:layout_marginTop="178dp" />

    <TextView
        android:id="@+id/activity_text1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="113dp"
        android:layout_below="@+id/activity_title"
        android:layout_alignLeft="@+id/activity_title"
        android:layout_alignStart="@+id/activity_title"
        android:layout_marginTop="30dp" />

    <TextView
        android:id="@+id/activity_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_alignTop="@+id/activity_text1"
        android:layout_alignLeft="@+id/activity_text1"
        android:layout_alignStart="@+id/activity_text1"
        android:layout_marginTop="66dp" />

</RelativeLayout>
